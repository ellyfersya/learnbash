read -p "bang lo mau install apa?" capekk

if command -v "$capekk" &> /dev/null; then
        echo "udah keinstall bangg"
                else

read -p "ga ada, lanjut install gak? [iya/gak]" permision

        case $permision in (y|Y|iya)
        apt-get install $capekk -y
;;
        *)
        echo "batalinaja"
;; esac
fi
